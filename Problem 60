#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a[5], b[5],ab[5];

    int i;

    printf("Please Enter your value of a:\n");
    for(i=0; i<5; i++){
      scanf("%d", &a[i]);
    }


    printf("Please Enter your value of b:\n");
    for(i=0; i<5; i++){
      scanf("%d", &b[i]);
    }

     for(i=0; i<5; i++){
        ab[i] = a[i] + b[i];
     }
      printf("value of ab array:\n");
     for(i=0; i<5; i++){
        printf("%d ", ab[i]);
     }



    return 0;
}
