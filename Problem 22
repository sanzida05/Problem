#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a;
    printf("Please enter your value:");

    scanf("%d",&a);

    switch(a){
    case 1:
        printf("Value is: 1\n");
        break;
    case 2:
        printf("Value is : 2\n");
         break;
    case 3:
        printf("Value is : 3\n");
         break;
    case 4:
        printf("Value is : 4\n");
         break;
    case 5:
        printf("Value is : 5\n");
         break;
    default:
        printf("Value is : unknown\n");

    }
    return 0;
}
