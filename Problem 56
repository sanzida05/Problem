#include <stdio.h>
#include <stdlib.h>

int main()
{
    int r[7];

  printf("Please Enter your value:\n");


int i;
  for(i=0; i<7; i++){

  scanf("%d\n",&r[i]);
  }

  for(i=0; i<7; i++){

  printf("%d\n",r[i]);
  }


  int w;
   w = r[0] + r[1] + r[3] + r[5] + r[6];

   printf("%d\n", r[w]);




    return 0;
}
