#include <stdio.h>
#include <stdlib.h>

int main()
{
    int x;
    printf("Enter the value of x:\n");

    scanf("%d",&x);

    if(x==1){
        printf("*\n");
        printf("**\n");
        printf("***\n");
    }
    else if(x==2){
        printf("Hi\n");
        printf("Hello\n");
        printf("Good morning\n");
    }
    else if(x==3){
        printf("ABCDE");
    }
    else{
        printf("error input");
    }

    return 0;
}
