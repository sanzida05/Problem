#include <stdio.h>
#include <stdlib.h>

int main()
{
  int a[5] = {12, 40, 36, 91, 74};

   printf("value of index 0: %d\n,"a[0]);
   printf("value of index 2: %d\n,"a[2]);
   printf("value of index 4: %d\n,"a[4]);



   //or

   int i;

   for(i=0; i<5; i++){
      printf("%d\n", a[i]);
   }

    return 0;
}
